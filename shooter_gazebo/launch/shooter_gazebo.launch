<launch>

<!-- Utiliza empty.world (ya escrito y largo) para facilitar las cosas, se cambia solo el 
    nombre del world que se launcheara-->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find shooter_description)/shooter_gazebo/worlds/shooter.world" />
    </include> 

    <!-- CODIGO CORRECTO 

    <!- - Convert a xacro into urdf and put on a parameter server - ->
    <param name="robot_description" command="$(find xacro)/xacro - -inorder '$(find shooter_description)/urdf/shooter.xacro' "/>  

    <!- - Spawn a robot into gazebo, << -model shooter >> is the name given to the robot- ->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model shooter" />
    
    -->
   <!-- CODIGO NUEVO --> 

  <!-- include our robots -->
  <include file="$(find shooter_description)/shooter_gazebo/launch/robots.launch"/>

</launch>